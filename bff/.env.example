NODE_ENV=development
HOST=0.0.0.0
PORT=8080
CORS_ORIGIN=*
JWT_SECRET=dev-secret-change-me
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=1 minute

# LLM provider (OpenAI/Claude/Local). For now, leave blank for mock behavior.
LLM_PROVIDER=mock
LLM_API_KEY=

# MinerU OCR service
MINERU_BASE_URL=
MINERU_API_KEY=
MINERU_MOCK=true

# Quota control (daily request budget)
DAILY_QUOTA=1000

# SQLite 默认保存在 prisma/dev.db，与 Prisma CLI 保持一致
DATABASE_URL=file:./dev.db
